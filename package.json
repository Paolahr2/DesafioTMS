{
  "name": "desafiotm",
  "version": "1.0.0",
  "description": "DesafioTM - Task Management Application",
  "scripts": {
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "test": "cd frontend && npm test",
    "install-frontend": "cd frontend && npm install",
    "build-backend": "cd backend && dotnet build",
    "start-backend": "cd backend/src/Presentation && dotnet run",
    "dev": "concurrently \"npm run start-backend\" \"npm run start\" --names \"backend,frontend\" --prefix name"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}